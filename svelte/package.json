{
  "name": "clicons-svelte",
  "version": "1.0.1",
  "description": "Clicons Svelte Icon Library",
  "homepage": "https://github.com/trikooo/clicons",
  "license": "MIT",
  "type": "module",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "svelte": "./dist/*.svelte",
      "default": "./dist/*.js"
    }
  },
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "sideEffects": false,
  "author": "Yacine Zendaoui",
  "scripts": {
    "generate": "node ./generate-icons.mjs",
    "build": "pnpm generate && svelte-package",
    "package": "svelte-package",
    "clean": "rm -rf dist src/icons src/index.ts",
    "publish:beta": "pnpm build && npm publish --tag beta --no-git-checks",
    "publish:prod": "pnpm build && npm publish --no-git-checks"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "svelte": "^5.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.2.2"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "keywords": [
    "icons",
    "svelte-icons",
    "svelte5",
    "ui-components",
    "design-system",
    "vector-icons",
    "svelte-component-library",
    "web-icons",
    "scalable-icons",
    "customizable-icons",
    "icon-library"
  ]
}
